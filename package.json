{
  "name": "dalenguyen",
  "version": "1.2.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --ssl",
    "prebuild": "ts-node -O '{\"module\": \"commonjs\"}' git.version.ts",
    "build": "npm run prebuild && ng build --prod --base-href 'https://dalenguyen.me' --aot --output-hashing=all && cp src/CNAME dist",
    "deploy:web": "npm run build && ngh --branch=master",
    "deploy:sentry": "sh sentry.sh",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "update:check": "ncu",
    "update:install": "ncu -u && npm i"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0-rc.7",
    "@angular/cdk": "^8.2.1",
    "@angular/common": "~9.0.0-rc.7",
    "@angular/compiler": "~9.0.0-rc.7",
    "@angular/core": "~9.0.0-rc.7",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~9.0.0-rc.7",
    "@angular/material": "^8.2.1",
    "@angular/platform-browser": "~9.0.0-rc.7",
    "@angular/platform-browser-dynamic": "~9.0.0-rc.7",
    "@angular/router": "~9.0.0-rc.7",
    "@sentry/browser": "^5.6.3",
    "@sentry/integrations": "^5.6.1",
    "buttercms": "^1.2.1",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.5.3",
    "smoothscroll-polyfill": "^0.4.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.7",
    "@angular/cli": "~9.0.0-rc.7",
    "@angular/compiler-cli": "~9.0.0-rc.7",
    "@angular/language-service": "~9.0.0-rc.7",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@types/jasmine": "~3.4.1",
    "@types/jasminewd2": "~2.0.7",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.2",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "typescript": "3.6.4"
  }
}
